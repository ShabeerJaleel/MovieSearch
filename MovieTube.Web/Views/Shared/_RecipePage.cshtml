@model object
<div >
    <div class="container-fluid"  >
        <div >
            @Html.Partial("_ThumbnailRecipe")
        </div>
    </div>
    <div class="row-fluid ">
        <div class="span2 offset5">
            <a ng-click="loadMore()" id="next-page" ng-show="settings.showMoreLink" class="animate">
                <button class="btn  btn-primary btn-large full-width">
                    Load more...
                </button>
            </a>
        </div>
    </div>
</div>
<div class="gap">
</div>

<script type="text/javascript">
    var initService = function () {
        var bootData = @Html.Raw(@Model);
        return{
            bootData : bootData
        };
    }; 
   
</script>
